<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1"
      minWidth="200" fx:controller="ca.bcit.cst.rongyi.ui.OptionPopupController">
    <children>
        <JFXButton fx:id="settingsButton" text="Settings" onAction="#openSettings" prefWidth="Infinity"></JFXButton>
        <JFXButton fx:id="aboutButton" text="About" onAction="#about" prefWidth="Infinity"></JFXButton>
        <Separator orientation="HORIZONTAL"></Separator>
        <JFXButton fx:id="exitButton" text="Exit" onAction="#exit" prefWidth="Infinity"></JFXButton>
    </children>
    <padding>
        <Insets top="5.0" bottom="5.0"/>
    </padding>
</VBox>
